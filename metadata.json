{
    "name": "leaf",
    "preprocessor": "sass",
    "widgetVersion": 2,
    "thumbnail": "thumbnail.png",
    "screenshot": "screenshot.jpg",
    "templateType": "standard",
    "pageTypes": ["home"],
    "images":[
        "images/feature.jpg"
    ],
    "colorSwatches":
    {
        "Swatch 1": {
            "color1":"#ffffff",
            "color2":"#FBE6A1",
            "color3":"#1e1e1e",
            "color4":"#1e1e1e",
            "color5":"#737373",
            "color6":"#919191",
            "color7":"#9AA7B5"
        }
    },
    "fontSwatch": {
       "title": "Acta & Questa Sans",
        "font1": {
            "font-family": "\"Playfair Display\", Times New Roman, serif",
            "font-size": "52px",
            "font-weight": "400",
            "line-height": "48px",
            "color": "@color-swatch4",
            "letter-spacing": "0px"
        },
        "font2": {
            "font-family": "\"Playfair Display\", Times New Roman, serif",
            "font-size": "30px",
            "font-weight": "400",
            "line-height": "40px",
            "color": "@color-swatch4",
            "letter-spacing": "0px"
        },
        "font3": {
            "font-family": "\"Playfair Display\", Times New Roman, serif",
            "font-size": "22px",
            "font-weight": "400",
            "line-height": "34px",
            "color": "@color-swatch4",
            "letter-spacing": "0px"
        },
        "font4": {
            "font-family": "\"Playfair Display\", Times New Roman, serif",
            "font-size": "20px",
            "font-weight": "400",
            "line-height": "38px",
            "color": "@color-swatch4",
            "letter-spacing": "0px"
        },
        "font5": {
            "font-family": "\"Rubik\", Geneva, \"Lucida Console\", sans-serif",
            "font-size": "19px",
            "font-weight": "400",
            "line-height": "36px",
            "color": "@color-swatch5",
            "letter-spacing": "0px"
        },
        "font6": {
            "font-family": "\"Rubik\", Geneva, \"Lucida Console\", sans-serif",
            "font-size": "28px",
            "font-weight": "400",
            "line-height": "36px",
            "color": "@color-swatch6",
            "color-hover": "@color-swatch6",
            "letter-spacing": "1px",
            "background-color": "@color-swatch1",
            "background-color-hover": "@color-swatch3"
        },
        "font7": {
            "font-family": "\"Rubik\", Geneva, \"Lucida Console\", sans-serif",
            "font-size": "15px",
            "font-weight": "400",
            "line-height": "26px",
            "color": "@color-swatch3",
            "letter-spacing": "2px",
            "background-color": "@color-swatch3",
            "background-color-hover": "@color-swatch3"
        },
        "font8": {
            "font-family": "\"Rubik\", Geneva, \"Lucida Console\", sans-serif",
            "font-size": "56px",
            "font-weight": "400",
            "line-height": "62px",
            "color": "@color-swatch1",
            "letter-spacing": "0px"
        },
        "font9": {
            "font-family": "\"Rubik\", Geneva, \"Lucida Console\", sans-serif",
            "font-size": "18px",
            "font-weight": "400",
            "line-height": "32px",
            "color": "@color-swatch1",
            "letter-spacing": "0px"
        },
        "font10": {
            "font-family": "\"Playfair Display\", Times New Roman, serif",
            "font-size": "19px",
            "font-weight": "400",
            "line-height": "30px",
            "color": "@color-swatch1",
            "letter-spacing": "2px"
        }
    },
    "styles": {
        "colors": {
            "color__primary": "#2B2B3C",
            "color__secondary": "#998D5E"
        },
        "site": {
            "color__body--background": "#ffffff",
            "color__container--background": "#ffffff",
            "color__link--text": "@color__secondary",
            "font__companyname--font-family": "@font-swatch10-font-family",
            "font__companyname--font-size": "24px",
            "font__companyname--line-height": "32px",
            "font__companyname--font-weight": "700",
            "font__companyname--letter-spacing": "0px",
            "color__companyname--text": "#2B2B3C"
        },
        "navigation": {
            "font__navigation--font-family": "@font-swatch5-font-family",
            "font__navigation--font-size": "18px",
            "font__navigation--font-weight": "400",
            "font__navigation--line-height": "22px",
            "font__navigation--letter-spacing": "0px",
            "color__navigation--background": "#ffffff",
            "color__navigation--text": "#2B2B3C",
            "color__navigation--text--hover": "#2B2B3C",
            "color__navigation--text--selected": "#2B2B3C"
        },
        "header": {
            "color__header--background": "#ffffff"
        },
        "footer": {
            "color__footer--background": "#ffffff"
        },
        "socialicons": {
            "color__socialicons--icon--background" : "#979fa1",
            "color__socialicons--icon" : "#fff"
        },
        "form": {
            "color__form-legend--text" : "#1e1e1e",
            "color__form-label--text" : "#969696",
            "color__form--background" : "#f5f5f5"
        },
        "image": {
            "color__image-caption--background": "rgba(0,0,0, 0.45)",
            "color__image-title--text": "#fff",
            "color__image-description--text": "#bebebe"
        },
        "gallery": {
            "color__gallery-caption--background" : "#fff",
            "color__gallery-title--text" : "@color__h2--text",
            "color__gallery-description--text" : "@color__p--text"
        }
    }
}